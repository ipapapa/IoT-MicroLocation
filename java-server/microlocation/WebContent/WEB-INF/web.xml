<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Microlocation</display-name>
  <servlet>
    <description></description>
    <display-name>BeaconController</display-name>
    <servlet-name>BeaconController</servlet-name>
    <servlet-class>microlocation.controllers.BeaconController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BeaconController</servlet-name>
    <url-pattern>/BeaconController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>WemoController</display-name>
    <servlet-name>WemoController</servlet-name>
    <servlet-class>microlocation.controllers.WemoController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WemoController</servlet-name>
    <url-pattern>/WemoController</url-pattern>
  </servlet-mapping>

  
  <listener>
  <listener-class>
  		microlocation.controllers.WemoListener
  </listener-class>
  </listener>
</web-app>
